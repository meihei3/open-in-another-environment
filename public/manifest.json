{
  "manifest_version": 2,
  "name": "open-in-another-environment",
  "version": "0.1.0",
  "description": "現在開いてるURLを別環境で開く",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}